<table class="table table-bordered table-condensed">
  <% @subjects.each do |subject| %>
    <tr>
      <td><%= subject.name %></td>
      <% subject.descriptions.each do |description| %>
        <td><%= description.marks.find_by_student_id(@student).try(:mark) %></td>
      <% end %>
    </tr>
  <% end %>
</table>
